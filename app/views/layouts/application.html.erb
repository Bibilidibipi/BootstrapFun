<!DOCTYPE html>
<html>
<head>
  <title>AuthDemo</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <button
        type="button"
        class="navbar-toggle"
        data-toggle="collapse"
        data-target="#links">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <spam class="icon-bar"></span>
      </button>
      <p class="navbar-brand">Links</p>
    </div>
    <div class="collapse navbar-collapse" id="links">
      <ul class="nav navbar-nav">
        <li><%= link_to "Sign Up", new_user_url %></li>
        <li><%= link_to "Log In", new_session_url %></li>
        <li><%= link_to "Home", static_pages_home_url %></li>
        <li><%= link_to "Contact", static_pages_contact_url %></li>
        <li><%= link_to "About", static_pages_about_url %></li>
      </ul>
    </div>
  </div>
</nav>

<%= render 'shared/errors' %>

<div class="container"><%= yield %></div>

</body>
</html>
